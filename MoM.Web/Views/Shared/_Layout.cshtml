<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta content="EasyModules.NET" name="application-name" />
    <meta content="#FFFFFF" name="msapplication-TileColor" />
    <meta content="~/site/favicon/mstile-144x144.png" name="msapplication-TileImage" />
    <meta content="~/site/favicon/mstile-70x70.png" name="msapplication-square70x70logo" />
    <meta content="~/site/favicon/mstile-150x150.png" name="msapplication-square150x150logo" />
    <meta content="~/site/favicon/mstile-310x150.png" name="msapplication-wide310x150logo" />
    <meta content="~/site/favicon/mstile-310x310.png" name="msapplication-square310x310logo" />
    <link href="~/site/favicon/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <link href="~/site/favicon/apple-touch-icon-57x57.png" rel="apple-touch-icon-precomposed" sizes="57x57" />
    <link href="~/site/favicon/apple-touch-icon-114x114.png" rel="apple-touch-icon-precomposed" sizes="114x114" />
    <link href="~/site/favicon/apple-touch-icon-72x72.png" rel="apple-touch-icon-precomposed" sizes="72x72" />
    <link href="~/site/favicon/apple-touch-icon-144x144.png" rel="apple-touch-icon-precomposed" sizes="144x144" />
    <link href="~/site/favicon/apple-touch-icon-60x60.png" rel="apple-touch-icon-precomposed" sizes="60x60" />
    <link href="~/site/favicon/apple-touch-icon-120x120.png" rel="apple-touch-icon-precomposed" sizes="120x120" />
    <link href="~/site/favicon/apple-touch-icon-76x76.png" rel="apple-touch-icon-precomposed" sizes="76x76" />
    <link href="~/site/favicon/apple-touch-icon-152x152.png" rel="apple-touch-icon-precomposed" sizes="152x152" />
    <link href="~/site/favicon/favicon-196x196.png" rel="apple-touch-icon-precomposed" sizes="196x196" type="image/png" />
    <link href="~/site/favicon/favicon-96x96.png" rel="apple-touch-icon-precomposed" sizes="96x96" type="image/png" />
    <link href="~/site/favicon/favicon-32x32.png" rel="apple-touch-icon-precomposed" sizes="32x32" type="image/png" />
    <link href="~/site/favicon/favicon-16x16.png" rel="apple-touch-icon-precomposed" sizes="16x16" type="image/png" />
    <link href="~/site/favicon/favicon-128.png" rel="apple-touch-icon-precomposed" sizes="128x128" type="image/png" />

    <title>EasyModules.NET - @ViewData["Title"]</title>
    <environment names="Development">
        <link rel="stylesheet" href="~/@ViewData["CssPath"]/app.css" />
        <link rel="stylesheet" href="~/css/font-awesome.css" />
    </environment>
    <environment names="Staging,Production">
        <link rel="stylesheet" href="~/@ViewData["CssPath"]/app.min.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/font-awesome.min.css" />
    </environment>
    @Html.ApplicationInsightsJavaScript(TelemetryConfiguration)
    @RenderSection("head", required: false)
</head>
<body>
    @RenderBody()
    @RenderSection("scripts", required: false)
</body>
</html>